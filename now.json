{
  "version": 2,
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/print/(.*)", "dest": "/print/$1" },
    { "src": "/page-data/(.*)", "dest": "/page-data/$1" },
    { "src": "/(.*).js", "dest": "/$1.js" },
    { "src": "/(.*).js.map", "dest": "/$1.js.map" },
    { "src": "/(.*).json", "dest": "/$1.json" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
